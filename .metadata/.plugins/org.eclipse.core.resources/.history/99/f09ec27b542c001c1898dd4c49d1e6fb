
public class Ejemplo10 {

	public static void main(String[] args) {
		
		//Caracter de control \n (salto de linea)
  System.out.println("Hay siete tipos de datos primitivos \nen Java\n\n\n");
  
  System.out.println("En primer lugar estan los booleanos...\n");
       //Caracter de control (tabulado)
  System.out.println("Nombre completo \t Edad");
  System.out.println("=============== \t ====");
  System.out.println("Felipe Martinez \t 40");
  System.out.println("Marta Dominguez \t 45");
  System.out.println("Luisa Fernandez \t 51\n");
  
    //Otros caracteres de control
  
   System.out.println("Para hacer una comversio≈Ñ \"explicita\" hay que...\n");
  
   System.out.println("Los caracteres de control empizan por \\");
  
  
  
	}

}
