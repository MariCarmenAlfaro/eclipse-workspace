
public class Ejemplo08 {

	public static void main(String[] args) {
	
	int asistentes = 5000;
	//Conversión implicita
	long personas = asistentes;
	
	System.out.println(personas);
	
	//Conversión implicita
	
	float gente = personas;
	
	System.out.println(gente);
	
	long jugadores = 1000;
	int players = jugadores; //cast
	}

}
